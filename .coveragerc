[run]
source = .
omit = 
    */venv/*
    */env/*
    */migrations/*
    manage.py
    */settings/*
    */wsgi.py
    */asgi.py
    */tests.py
    */test_*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:

[html]
directory = htmlcov